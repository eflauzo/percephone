{
  "entrypoint": "index.html",
  "shell": "src/my-app.html",
  "fragments": [
    "src/my-app.html",
    "src/my-station.html",
    "src/my-stationstatus.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower_components/when/*.js",
    "bower_components/autobahn/*.js",
    "bower.json",
    "service-worker.js"
  ],
  "extraDependencies": [
    "manifest.json",
    "bower_components/webcomponentsjs/*.js",
    "bower_components/when/*.js",
    "bower_components/autobahn/*.js"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [{
    "name": "bundled",
    "bundle": false,
    "js": {"minify": false},
    "css": {"minify": false},
    "html": {"minify": false}
  }]
}
